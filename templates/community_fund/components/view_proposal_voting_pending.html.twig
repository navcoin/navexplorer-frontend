<div class="margin-bottom row">
    <div class="col-md-6 col-sm-12 margin-bottom">
        <h3>Voting cycle {{ votingCycles.latestVotes.cycle }}</h3>
        {{ proposalVoteProgress(proposal, blockCycle, votingCycles) }}
        <p class="text-muted">Accept {{ votingCycles.latestVotes.yes }} / Reject {{ votingCycles.latestVotes.no }}</p>
    </div>

    <div class="col-md-6 col-sm-12">
        <h3>Voting Trend</h3>
        <canvas id="trendChart" class="margin-top"></canvas>
    </div>
</div>

